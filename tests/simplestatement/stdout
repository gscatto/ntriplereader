ntriplebuilder_clearstatement();
ntriplebuilder_clearsubject();
ntriplebuilder_adduri("SUBJECT");
ntriplebuilder_buildsubject();
ntriplebuilder_clearpredicate();
ntriplebuilder_adduri("PREDICATE");
ntriplebuilder_buildpredicate();
ntriplebuilder_clearobject();
ntriplebuilder_adduri("OBJECT");
ntriplebuilder_buildobject();
ntriplebuilder_buildstatement();
